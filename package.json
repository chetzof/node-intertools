{
  "name": "node-intertools",
  "version": "0.0.0-development",
  "description": "An npm package for demonstration purposes using TypeScript to build for both the ECMAScript Module format (i.e. ESM or ES Module) and CommonJS Module format. It can be used in Node.js and browser applications.",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "vitest",
    "semantic-release": "semantic-release",
    "prepack": "npm run build",
    "fix": "eslint --fix --ext .ts ./",
    "index": "ts-node src/index.ts",
    "serve": "ts-node src/server.ts"
  },
  "bin": "./lib/cjs/server.js",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chetzof/node-intertools.git"
  },
  "keywords": [
    "npm",
    "javascript",
    "typescript",
    "esm",
    "cjs",
    "nodejs",
    "commonjs",
    "ecmascript",
    "beginner",
    "example",
    "demonstration"
  ],
  "author": "Chetzof",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chetzof/node-intertools/issues"
  },
  "homepage": "https://github.com/chetzof/node-intertools#readme",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "got": "^12.5.1",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "@fastify/reply-from": "^8.3.0",
    "@mswjs/interceptors": "^0.17.5",
    "abstract-cache": "^1.0.1",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "fastify": "^4.6.0",
    "find-cache-dir": "^3.3.2",
    "msw": "^0.47.3",
    "run-middleware": "^0.9.10"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "packageManager": "yarn@3.2.3"
}
